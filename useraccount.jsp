		        <form class="form-register" id="form-register" method="post">

                    
		        
		        	<div id="form-total">

                        <h2>

			            <center><span style="color: white;" class="step-text" >UPDATE OR DELETE PRODUCT DETAILS </span></center>

			            
			            </h2>	
			            
			      
			            <section>
			            
			                <div class="inner">
			                
			                	<h3></h3>
			                	
								<div class="form-row table-responsive">
								
									<table class="table">
									
									<!-- create a loop -->
									
										<c:forEach var="pro" items="${proDetails}"><!-- get values from proDetails attribute -->
	
											<c:set var="productID" value="${pro.productID}"/><!-- assign values to variables -->
											<c:set var="productname" value="${pro.productname}"/>
											<c:set var="description" value="${pro.description}"/>
											<c:set var="image" value="${pro.image}"/>
											<c:set var="price" value="${pro.price}"/>
	
										<tbody>
										
											<tr class="space-row">
												<th>Product ID</th>
												<td>${pro.productID}</td>
											</tr>
											
											<tr class="space-row">
												<th>Product Name</th>
												<td>${pro.productname}</td>
											</tr>
											
											<tr class="space-row">
												<th>Description</th>
												<td>${pro.description}</td>
											</tr>
											
											<tr class="space-row">
												<th>Image</th>
												<td>${pro.image}</td>
											</tr>
											
											<tr class="space-row">
												<th>Price</th>
												<td>${pro.price}</td>
											</tr>
											
										</tbody>
										
										</c:forEach>
										
									</table>
									
								</div>
								
		<c:url value="updateproducts.jsp" var="proupdate">
								
		<c:param name="id" value="${productID}"></c:param>
		<c:param name="pname" value="${productname}"></c:param>
		<c:param name="des" value="${description}"></c:param>
		<c:param name="img" value="${image}"></c:param>
		<c:param name="pri" value="${price}"></c:param>
		
		</c:url>
	
	
		<a href="${proupdate}">

        			<center><input type="button" name="update" value="Update Product Details" class="default-button"></center>
	
		</a>

	<br><br><br>
	
